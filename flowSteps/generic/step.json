{
	"label": "generic",
	"name": "generic",
	"category": "integrations",
	"description": "Sends generic request",
	"inputs": [
		{
			"label": "Method",
			"name": "method",
			"type": "choice",
			"description": "Select method",
			"defaultValue": "get",
			"required": "true",
			"options": {
				"possibleValues": [
					{
						"label": "Get",
						"name": "get"
					},
					{
						"label": "Post",
						"name": "post"
					},
					{
						"label": "Put",
						"name": "put"
					},
					{
						"label": "Delete",
						"name": "delete"
					},
					{
						"label": "Connect",
						"name": "connect"
					},
					{
						"label": "Head",
						"name": "head"
					},
					{
						"label": "Options",
						"name": "options"
					},
					{
						"label": "Patch",
						"name": "patch"
					},
					{
						"label": "Trace",
						"name": "trace"
					}
				],
				"allowContextSelector": "false"
			}
		},
		{
			"label": "Url",
			"name": "url",
			"type": "text",
			"description": "This is used to set a external URL",
			"required": "true",
			"options": {
				"allowContextSelector": "false"
			}
		},
		{
			"label": "Body",
			"name": "body",
			"type": "text",
			"required": "['post', 'put', 'patch'].includes(config.method)",
			"visibility": "['post', 'put', 'patch'].includes(config.method)",
			"description": "This is used to set body request",
			"options": {
				"allowContextSelector": "false"
			}
		},
		{
			"label": "Required call back",
			"name": "config.requiredCallBack",
			"type": "boolean",
			"description": "True if required call back",
			"defaultValue": false,
			"options": {
				"allowContextSelector": "false"
			}
		},
		{
			"label": "CallbackData",
			"name": "callbackData",
			"type": "text",
			"description": "This is used to set CallbackData",
			"required": "config.requiredCallBack === true",
			"visibility": "config.requiredCallBack === true",
			"options": {
				"allowContextSelector": "false"
			}
		},
		{
			"label": "Callbacks",
			"name": "callbacks",
			"type": "text",
			"description": "This is used to set callbacks",
			"required": "config.requiredCallBack",
			"visibility": "config.requiredCallBack",
			"options": {
				"allowContextSelector": "false"
			}
		}
	],
	"outputs": [
		{
			"label": "Response",
			"name": "response",
			"type": "text",
			"description": "The response to external URL"
		}
	]
}